                                <h5>@item.ad_price</h5>
                            </div>
                        </div>
                        <!-- Hover Effects -->
                        <div class="hover-effects">
                            <div class="text">
                                <h6>@item.ad_name</h6>
                                <h5>@item.ad_price</h5>
                                <p>@item.ad_summery</p>
                            </div>
                            <a href="#" class="btn project-btn">جزییات بیشتر<i class="fa fa-long-arrow-right" aria-hidden="true"></i></a>
                        </div>
                    </div>
                        break;
                    }
                }
            }
        </div>

    </section>

}
